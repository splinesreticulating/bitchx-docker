   echo $G $(hblk)[$(hwht) 1$(hblk)]$(cl) $cparse("$(c6)<%nshiftee$(c6)>%n hi")
   echo $G $(hblk)[$(hwht) 2$(hblk)]$(cl) $cparse("%b<%nshiftee%b>%n hi")
   echo $G $(hblk)[$(hwht) 3$(hblk)]$(cl) $cparse("%C<%nshiftee%C>%n hi")
   echo $G $(hblk)[$(hwht) 4$(hblk)]$(cl) $cparse("%W<%Bshiftee%W>%n hi")
   echo $G $(hblk)[$(hwht) 5$(hblk)]$(cl) $cparse("%K<%Wshiftee%K>%n hi")
   echo $G
   echo $G usage$(hblk):$(cl) /pformat $(hblk)[$(cl)#$(hblk)]
   echo $G current public format$(hblk):$(cl) $(hblk)[$(hwht)$(pub)$(hblk)]
 }
