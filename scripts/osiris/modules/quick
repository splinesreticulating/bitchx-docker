alias mformat {
   if ([$0] && ([$0] <= mfo)) {
   @msgs=[$0]
   ^load ~/osiris/formats/msgs/msgs$(msgs)
   echo $G loaded msg format $(hblk)[$(hwht)$(msgs)$(hblk)]
 }{
   echo $G available msg formats$(hblk):
   echo $G
   echo $G $(hblk)[$(hwht) 1$(hblk)]$(cl) $cparse("%K-> %C(%cnick%C)%n hi %K/%n %Cnick%K(%cuser@host%K):%n hi")
   echo $G $(hblk)[$(hwht) 2$(hblk)]$(cl) $cparse("%C[%Wnick%C]%n hi %K/%n %C[%Wnick%r!%Wuser@host%C]%n hi")
   echo $G $(hblk)[$(hwht) 3$(hblk)]$(cl) $cparse("%B[%cm%Csg%B(%Cnick%B)]%n hi %K/%n %B[%Cnick%B(%cuser@host%B)]%n hi")
   echo $G $(hblk)[$(hwht) 4$(hblk)]$(cl) $cparse("%b-%B¯ %K(%Gnick%K)%n hi %K/%n %B®%b- %K(%Gnick %Cí %guser@host%K)%n hi")
   echo $G $(hblk)[$(hwht) 5$(hblk)]$(cl) $cparse("%K[%4%Wnick%n%K]%n hi %K/%n %K[%4%Wnick%4%C!%w%4user@host%n%K]%n hi")
   echo $G $(hblk)[$(hwht) 6$(hblk)]$(cl) $cparse("%K[%rmsg%K(%Rnick%K)]%n hi %K/%n %K[%Pnick%K(%puser@host%K)]%n hi")
   echo $G
   echo $G usage$(hblk):$(cl) /mformat $(hblk)[$(cl)#$(hblk)]
   echo $G current msg format$(hblk):$(cl) $(hblk)[$(hwht)$(msgs)$(hblk)]
 }
   if ([$0] > mfo) {echo $G number specified is out of range!}
}

alias nformat {
   if ([$0] && ([$0] <= nfo)) {
   @ncomp=[$0]
   ^load ~/osiris/formats/ncomp/ncomp$(ncomp)
   echo $G loaded nick completion format $(hblk)[$(hwht)$(ncomp)$(hblk)]
 }{
   echo $G available nick completion formats$(hblk):
   echo $G
   echo $G $cparse("%K[%W 1%K]%n nick: hi   %K[%W11%K]%n nick: hi   %K[%W21%K]%n nick: hi   %K[%W31%K]%n nick: hi")
   echo $G $cparse("%K[%W 2%K]%n nick, hi   %K[%W12%K]%n nick, hi   %K[%W22%K]%n nick, hi   %K[%W32%K]%n nick, hi")
   echo $G $cparse("%K[%W 3%K]%n nick. hi   %K[%W13%K]%n nick. hi   %K[%W23%K]%n nick. hi   %K[%W33%K]%n nick. hi")
   echo $G $cparse("%K[%W 4%K]%n nick+ hi   %K[%W14%K]%n nick+ hi   %K[%W24%K]%n nick+ hi   %K[%W34%K]%n nick+ hi")
   echo $G $cparse("%K[%W 5%K]%n nick- hi   %K[%W15%K]%n nick- hi   %K[%W25%K]%n nick- hi   %K[%W35%K]%n nick- hi")
   echo $G $cparse("%K[%W 6%K]%n :nick: hi  %K[%W16%K]%n :nick: hi  %K[%W26%K]%n :nick: hi  %K[%W36%K]%n :nick: hi")
   echo $G $cparse("%K[%W 7%K]%n [nick] hi  %K[%W17%K]%n [nick] hi  %K[%W27%K]%n [nick] hi  %K[%W37%K]%n [nick] hi")
   echo $G $cparse("%K[%W 8%K]%n (nick): hi %K[%W18%K]%n \(nick\): hi %K[%W28%K]%n \(nick\): hi %K[%W38%K]%n \(nick\): hi")
   echo $G $cparse("%K[%W 9%K]%n nick - hi  %K[%W19%K]%n nick - hi  %K[%W29%K]%n nick - hi  %K[%W39%K]%n nick - hi")
   echo $G $cparse("%K[%W10%K]%n nick= hi   %K[%W20%K]%n nick= hi   %K[%W30%K]%n nick= hi   %K[%W40%K]%n nick= hi")
   echo $G
   echo $G usage$(hblk):$(cl) /nformat $(hblk)[$(cl)#$(hblk)]
   echo $G current nick completion format$(hblk):$(cl) $(hblk)[$(hwht)$(ncomp)$(hblk)]
 }
   if ([$0] > nfo) {echo $G number specified is out of range!}
}

alias noformat {
