@afo=[7]
@cfo=[3]
@efo=[21]
@ffo=[3]
@kfo=[4]
@mfo=[6]
@nfo=[40]
@nofo=[2]
@pfo=[5]
@scfo=[7]
@snfo=[2]
@spfo=[5]
@sbfo=[13]
@wfo=[3]

alias aformat {
   if ([$0] && ([$0] <= afo)) {
   @away=[$0]
   ^load ~/osiris/formats/away/away$(away)
   echo $G loaded away format $(hblk)[$(hwht)$(away)$(hblk)]
 }{
   echo $G away formats available$(hblk):
   echo $G
   echo $G $(hblk)[$(hwht) 1$(hblk)]$(cl) shiftee is away, reason \(l!on\) : os/bx
   echo $G $(hblk)[$(hwht) 2$(hblk)]$(cl) shiftee is detached. reason \(log!os.bx\)
   echo $G $(hblk)[$(hwht) 3$(hblk)]$(cl) shiftee is away!reason [osiris!log]
   echo $G $(hblk)[$(hwht) 4$(hblk)]$(cl) shiftee is away\(reason\) log\(yes\) os\($(osrel)\)
   echo $G $(hblk)[$(hwht) 5$(hblk)]$(cl) shiftee is idle/reason - log/os.bx
   echo $G $(hblk)[$(hwht) 6$(hblk)]$(cl) shiftee is away, reason \(l!on\) [os/bx]
   echo $G $(hblk)[$(hwht) 7$(hblk)]$(cl) shiftee is away: \(reason\) [BX-MsgLog ON]
   echo $G
   echo $G usage$(hblk):$(cl) /aformat $(hblk)[$(cl)#$(hblk)]
   echo $G current away format$(hblk):$(cl) $(hblk)[$(hwht)$(away)$(hblk)]
 }
   if ([$0] > afo) {echo $G number specified is out of range!}
}

alias cformat {
   if ([$0] && ([$0] <= cfo)) {
   @cdcc=[$0]
   ^load ~/osiris/formats/cdcc/cdcc$(cdcc)
   echo $G loaded cdcc plist format $(hblk)[$(hwht)$(cdcc)$(hblk)]
 }{
   echo $G cdcc plist formats available$(hblk):
   echo $G
   echo $G $(hblk)[$(hwht) 1$(hblk)]$(cl) \(os!dcc\) /ctcp shiftee cdcc send N to get N
   echo $G      ::: # 1 \(34.61kb [0x]\) this is a file
   echo $G              . this is a note
   echo $G      \(os!dcc\) offered/34.61kb . sent/0b
   echo $G
   echo $G $(hblk)[$(hwht) 2$(hblk)]$(cl)    [os/cdcc % 1 pack offered \(34.61kb\)]
   echo $G     Þ1ÝÄ\( 0 gets - 34.61kb \)Ä> this is a file
   echo $G      ÀÄÄÄ>  this is a note
   echo $G         [0b sent, record 0b/s, 5/5 open]
   echo $G
   echo $G $(hblk)[$(hwht) 3$(hblk)]$(cl) [os%cdcc] /ctcp sh|ftee cdcc help for commands
   echo $G      ... \(0x/34.61kb\) #1 - this is a file
   echo $G         øùú -  this is a note
   echo $G      [os%cdcc] offered/34.61kb . sent/0b
   echo $G 
   echo $G usage$(hblk):$(cl) /cformat $(hblk)[$(cl)#$(hblk)]
   echo $G current cdcc plist format$(hblk):$(cl) $(hblk)[$(hwht)$(cdcc)$(hblk)]
 }
   if ([$0] > cfo) {echo $G number specified is out of range!}
}

alias eformat {
   if ([$0] && ([$0] <= efo)) {
   @echo=[$0]
   ^load ~/osiris/formats/echo/echo$(echo)
   echo $G loaded echo string format $(hblk)[$(hwht)$(echo)$(hblk)]
 }{
   echo $G available echo string formats$(hblk):
   echo $G
   echo $G $(hblk)[$(hwht) 1$(hblk)]$(cl) $cparse("%Kú%wú%Wú   %K[%W11%K]%n %Kú%r-%RÄ   %K[%W21%K]%n %Rù%Wí%Rù")
   echo $G $(hblk)[$(hwht) 2$(hblk)]$(cl) $cparse("%Kú%rú%Rú   %K[%W12%K]%n %B:%c:%b:")
   echo $G $(hblk)[$(hwht) 3$(hblk)]$(cl) $cparse("%Kú%gú%Gú   %K[%W13%K]%n %c­%C!%c­")
   echo $G $(hblk)[$(hwht) 4$(hblk)]$(cl) $cparse("%Kú%bú%Bú   %K[%W14%K]%n %K=%b=%B=")
   echo $G $(hblk)[$(hwht) 5$(hblk)]$(cl) $cparse("%Kú%mú%Mú   %K[%W15%K]%n %KÄ%cÄ%CÄ")
   echo $G $(hblk)[$(hwht) 6$(hblk)]$(cl) $cparse("%Kú%cú%Cú   %K[%W16%K]%n %KÜ%wÜ%WÜ")
   echo $G $(hblk)[$(hwht) 7$(hblk)]$(cl) $cparse("%B[%G!%B]   %K[%W17%K]%n %W®%Bë%W¯")
   echo $G $(hblk)[$(hwht) 8$(hblk)]$(cl) $cparse("%r(%Bð%r)   %K[%W18%K]%n %Kø%Rù%rú")
   echo $G $(hblk)[$(hwht) 9$(hblk)]$(cl) $cparse("%n***   %K[%W19%K]%n %K[%wo%Ws%K]")
   echo $G $(hblk)[$(hwht)10$(hblk)]$(cl) $cparse("%g-%G=%g-   %K[%W20%K]%n %K[%C%%%K]")
   echo $G
   echo $G usage$(hblk):$(cl) /eformat $(hblk)[$(cl)#$(hblk)]
   echo $G current echo string format$(hblk):$(cl) $(hblk)[$(hwht)$(echo)$(hblk)]
 }
   if ([$0] > efo) {echo $G number specified is out of range!}
}

alias fformat {
   if ([$0] && ([$0] <= ffo)) {
   @fsets=[$0]
   ^load ~/osiris/formats/fsets/fsets$(fsets)
   echo $G loaded fsets format $(hblk)[$(hwht)$(fsets)$(hblk)]
 }{
   echo $G available fsets formats$(hblk):
   echo $G
   echo $G $(hblk)[$(hwht) 1$(hblk)]$(cl) $G $(c1)join$(c6)!$(c2)#channel $(c6)-> $(c5)nick$(c6)\($(c3)user@host$(c6)\)
   echo $G $(hblk)[$(hwht) 2$(hblk)]$(cl) $G $(hcyn)nick $(hblk)[$(cyn)user@host$(hblk)]$(cl) has joined #channel
   echo $G $(hblk)[$(hwht) 3$(hblk)]$(cl) $G $(c1)join$(c6)\($(c2)#channel$(c6)\)$(c7) nick $(c6)\($(c2)user@host$(c6)\)
   echo $G
   echo $G usage$(hblk):$(cl) /fformat $(hblk)[$(cl)#$(hblk)]
   echo $G current fsets format$(hblk):$(cl) $(hblk)[$(hwht)$(fsets)$(hblk)]
 }
   if ([$0] > ffo) {echo $G number specified is out of range!}
}

