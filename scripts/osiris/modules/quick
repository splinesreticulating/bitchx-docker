   echo $G $(hblk)[$(hwht) 2$(hblk)]$(cl) $cparse("%K-%Pnick%K(%puser@host%K)-%n hi")
   echo $G
   echo $G usage$(hblk):$(cl) /noformat $(hblk)[$(cl)#$(hblk)]
   echo $G current notice format$(hblk):$(cl) $(hblk)[$(hwht)$(nots)$(hblk)]
 }
