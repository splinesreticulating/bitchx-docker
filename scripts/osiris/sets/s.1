alias fix.scn {
        if ([$[1]0] == [@]) {
                @ nick = right(${strlen($0) - 1} $0)
                @ function_return = [$(hc1)@$(lc1)$[9]nick$(hc2)]
        }{
        if ([$[1]0] == [+]) {
                @ nick = right(${strlen($0) - 1} $0)
                @ function_return = [$(hc1)+$(lc1)$[9]nick$(hc2)]
        }{
                @ function_return = [ $(lc1)$[9]0$(hc2)]
        }}
}

alias scan {
        if ([$0]) {@ chan = [$0]}{@ chan = [$C]}
        @ voc = 0;@ ops = 0;@ non = 0
        fe ($chanusers($chan)) nick {
                if (ischanop($nick $chan) == 1) {
                        @ push(nicks.ops @$nick)
                        @ ops = ops + 1
                }{
                if (isvoice($chan $nick) == 1) {
                        @ push(nicks.voc +$nick)
                        @ voc = voc + 1
                }{
                        @ push(nicks.non $nick);@ non = non + 1
