echo    $(sc5)³   $(sc3)[$fix.scn($pad(10))] [$fix.scn($[10]0)] [$fix.scn($pad(10))] [$fix.scn($[10]1)] [$fix.scn($pad(10))]  $(sc5)³
}
alias asayscan3nicks {
echo    $(sc5)³   $(sc3)[$fix.scn($pad(10))] [$fix.scn($[10]0)] [$fix.scn($[10]1)] [$fix.scn($[10]2)] [$fix.scn($pad(10))]  $(sc5)³ 
}
alias asayscan4nicks {
echo    $(sc5)³   $(sc3)[$fix.scn($[10]0)] [$fix.scn($[10]1)] [$fix.scn($pad(10))] [$fix.scn($[10]2)] [$fix.scn($[10]3)]  $(sc5)³
