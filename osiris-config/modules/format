}

alias format_echo_menu {
purge line
@ lines = 21
@ lmenu = [echo]
@ line.0 = [[ 1]$(cl) $cparse("%Kú%wú%Wú")]
@ line.1 = [[ 2]$(cl) $cparse("%Kú%rú%Rú")]
@ line.2 = [[ 3]$(cl) $cparse("%Kú%gú%Gú")]
@ line.3 = [[ 4]$(cl) $cparse("%Kú%bú%Bú")]
@ line.4 = [[ 5]$(cl) $cparse("%Kú%mú%Mú")]
@ line.5 = [[ 6]$(cl) $cparse("%Kú%cú%Cú")]
@ line.6 = [[ 7]$(cl) $cparse("%B[%G!%B]")]
@ line.7 = [[ 8]$(cl) $cparse("%r(%Bð%r)")]
@ line.8 = [[ 9]$(cl) $cparse("%n***")]
@ line.9 = [[10]$(cl) $cparse("%g-%G=%g-")]
@ line.10 = [[11]$(cl) $cparse("%Kú%r-%RÄ")]
@ line.11 = [[12]$(cl) $cparse("%B:%c:%b:")]
@ line.12 = [[13]$(cl) $cparse("%c­%C!%c­")]
@ line.13 = [[14]$(cl) $cparse("%K=%b=%B=")]
@ line.14 = [[15]$(cl) $cparse("%KÄ%cÄ%CÄ")]
@ line.15 = [[16]$(cl) $cparse("%KÜ%wÜ%WÜ")]
@ line.16 = [[17]$(cl) $cparse("%W®%Bë%W¯")]
@ line.17 = [[18]$(cl) $cparse("%Kø%Rù%rú")]
@ line.18 = [[19]$(cl) $cparse("%K[%wo%Ws%K]")]
@ line.19 = [[20]$(cl) $cparse("%K[%C%%%K]")]
@ line.20 = [[21]$(cl) $cparse("%Rù%Wí%Rù")]
@ line.21 = [[22]$(cl) back to formats menu]
@ lhilite = 0
display
}

