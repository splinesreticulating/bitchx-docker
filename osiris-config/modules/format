alias nosave_format {
e1
input "quit without saving [N/y]? " if ([$0]==[Y]) {restore_keys;window kill;^wsetupdate;echo $G osir$(cyn)i$(cl)s formats $(cyn)NOT$(cl) saved.}{e2;formats_menu}}

^fmenu
