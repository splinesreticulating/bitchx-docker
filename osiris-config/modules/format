           (8) {ncomp39_load}
           (9) {ncomp40_load}
           (10) {format_ncomp_menu}
         }}
      (nots) {
         switch ($lhilite) {
           (0) {nots1_load}
           (1) {nots2_load}
           (2) {formats_menu}
         }}
      (pub) {
         switch ($lhilite) {
           (0) {pub1_load}
