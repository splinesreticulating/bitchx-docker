@ line.1 = [[ 2]$(cl) $G $(hcyn)nick $(hblk)[$(cyn)user@host$(hblk)]$(cl) has joined #channel]
@ line.2 = [[ 3]$(cl) $G $(c1)join$(c6)\($(c2)#channel$(c6)\)$(c7) nick $(c6)\($(c2)user@host$(c6)\)$(cl)]
@ line.3 = [[ 4]$(cl) back to formats menu]
@ lhilite = 0
