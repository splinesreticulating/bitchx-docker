### fix by suicide
alias fix.size {
  ^local sz.
  @sz.s=[$0]
  ^stack push set floating_point_math_var
  ^set floating_point_math_var on
  @function_return=sz.s==0 ? [0b] : sz.s<1024 && sz.s > 0 ? sz.s ## [b] : sz.s >= 1024 && sz.s < 1048576 ? trunc(2 ${sz.s/1024}) ## [kb] : sz.s >= 1048576 ? trunc(2 ${sz.s/1048576}) ## [mb] :
  ^stack pop set floating_point_math_var
}
